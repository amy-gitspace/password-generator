import random
def pass_func(passlen):
    s = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!"#$%&'()*+,-./:;<=>?@[\]^_`{|}~"
    # passlen = int(input("please input the password length: "))
    password =  "".join(random.sample(s,passlen ))
    print(password)

pass_func(int(input("please input the password length: ")))

